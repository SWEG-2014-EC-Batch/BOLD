//a program to calculate the hypotenuse of a right angle triangle
#include<iostream>
#include<math.h>//icludung the math library in orderto use pow and sqrtSS
using namespace std;

float hypotenuse(double side1,double side2);//declare the function
float hypotenuse(double side1,double side2){
    double squareside3,side3;
    squareside3=pow(side1,2)+pow(side2,2);//calculate the square of the hypotenuse

    side3=sqrt(squareside3);//find the square root of the squareside3
    return side3;
   
}
